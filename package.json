{
  "name": "react-tailwind-flowbite-parcel-template",
  "version": "1.0.0-alpha",
  "description": "React Tailwind Flowbite Parcel Template",
  "author": "Mark <mark@remarkablemark.org>",
  "source": "src/index.html",
  "scripts": {
    "build": "parcel build",
    "clean": "rm -rf .parcel-cache dist",
    "lint": "eslint --ignore-path .gitignore .",
